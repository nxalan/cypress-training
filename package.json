{
  "name": "survey-react",
  "version": "2.0.0",
  "description": "Survey React SPA",
  "author": "Alan Nicolas de Oliveira",
  "homepage": "https://enquestesreact.herokuapp.com/",
  "license": "GNU GPL3",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev:base": "webpack-dev-server --open --config webpack.dev.js",
    "dev": "npm run dev:base -- --open",
    "build": "webpack --config webpack.prod.js",
    "check": "npm-check -u -s",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu npx jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.7.0",
    "eslint": "^7.9.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-standard": "^5.0.0",
    "full-icu": "^1.4.0",
    "git-commit-msg-linter": "^4.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-localstorage-mock": "^2.4.19",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "typescript": "^4.0.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  }
}
